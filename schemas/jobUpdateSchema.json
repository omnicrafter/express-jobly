{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["job title"]
    },
    "salary": {
      "type": "integer",
      "default": 0,
      "title": "The salary Schema",
      "examples": [400]
    },
    "equity": {
      "type": "number",
      "default": 0.0,
      "title": "The equity Schema",
      "examples": [0.4]
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "title": "job title",
      "salary": 400,
      "equity": 0.4
    }
  ]
}

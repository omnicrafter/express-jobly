{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["job"]
    },
    "minSalary": {
      "type": "integer",
      "default": 0,
      "title": "The minSalary Schema",
      "examples": [400]
    },
    "hasEquity": {
      "type": "boolean",
      "default": false,
      "title": "The hasEquity Schema",
      "examples": [true]
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "title": "job",
      "minSalary": 400,
      "hasEquity": true
    }
  ]
}
